### Properties #######
######################

#Pessoa TCU
mp:mapPropertyPessoaTCU01
	a r2r:PropertyMapping;
	r2r:prefixDefinitions "foaf: <http://xmlns.com/foaf/0.1/> . tcu: <http://arida.ufc.br/vocab/tcu/> . vocab: <http://arida.ufc.br/vocab/ ." ;
	r2r:sourcePattern "?SUBJ foaf:name ?o";
	r2r:mappingRef mp:pessoaTCUMappingClass ;
	r2r:targetPattern "?SUBJ foaf:name ?o" .
	

mp:mapPropertyPessoaTCU02
	a r2r:PropertyMapping;
	r2r:prefixDefinitions "dbr: <http://dbpedia.org/resource/> . tcu: <http://arida.ufc.br/vocab/tcu/> . vocab: <http://arida.ufc.br/vocab/ ." ;
	r2r:sourcePattern "?SUBJ dbr:cpf ?o";
	r2r:mappingRef mp:pessoaTCUMappingClass ;
	r2r:targetPattern "?SUBJ dbr:cpf ?o" .

#Restricao TCU

