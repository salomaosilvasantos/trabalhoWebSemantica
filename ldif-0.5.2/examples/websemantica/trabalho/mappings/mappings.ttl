	a r2r:PropertyMapping;
	r2r:prefixDefinitions "tcu: <http://arida.ufc.br/vocab/tcu/> . vocab: <http://arida.ufc.br/vocab/ ." ;
	r2r:sourcePattern "?SUBJ tcu:numeroProcess ?o";
	r2r:mappingRef mp:restricaoTCUMappingClass ;
	r2r:targetPattern "?SUBJ vocab:numeroProcess ?o" .
	
