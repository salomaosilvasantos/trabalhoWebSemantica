	a r2r:PropertyMapping;
	r2r:prefixDefinitions "foaf: <http://xmlns.com/foaf/0.1/> . vocab: <http://arida.ufc.br/vocab/ ." ;
	r2r:sourcePattern "?SUBJ foaf:state ?uf";
	r2r:mappingRef mp:restricaoTCUMappingClass ;
	r2r:targetPattern "?SUBJ vocab:state ?uf" .
	
