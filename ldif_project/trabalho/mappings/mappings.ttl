### Properties #######
######################



#Pessoa
mp:mapPropertyPessoa01
	a r2r:PropertyMapping;
	r2r:prefixDefinitions "foaf: <http://xmlns.com/foaf/0.1/> . vocab: <http://arida.ufc.br/vocab/ ." ;
	r2r:sourcePattern "?SUBJ foaf:name ?o";
	r2r:mappingRef mp:pessoaMappingClass ;
	r2r:targetPattern "?SUBJ foaf:name ?o" .
	
mp:mapPropertyPessoa02
	a r2r:PropertyMapping;
	r2r:prefixDefinitions "dbr: <http://dbpedia.org/resource/>. vocab: <http://arida.ufc.br/vocab/ ." ;
	r2r:sourcePattern "?SUBJ dbr:cpf ?o";
	r2r:mappingRef mp:pessoaMappingClass ;
	r2r:targetPattern "?SUBJ dbr:cpf ?o" .
	
	
